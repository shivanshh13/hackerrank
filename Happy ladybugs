#include <stdio.h>
#include <string.h>

char* happyLadybugs(char b[]) {
    int freq[26] = {0}, n = strlen(b), i, hasEmpty = 0;

    for(i = 0; i < n; i++) {
        if(b[i] == '_') hasEmpty = 1;
        else freq[b[i] - 'A']++;
    }

    for(i = 0; i < 26; i++)
        if(freq[i] == 1) return "NO";

    if(hasEmpty) return "YES";

    for(i = 0; i < n; i++)
        if((i == 0 || b[i] != b[i-1]) && (i == n-1 || b[i] != b[i+1]))
            return "NO";

    return "YES";
}

int main() {
    int g, n;
    scanf("%d", &g);
    while(g--) {
        scanf("%d", &n);
        char b[105];
        scanf("%s", b);
        printf("%s\n", happyLadybugs(b));
    }
    return 0;
}
