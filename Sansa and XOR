#include <stdio.h>

int sansaXor(int arr[], int n) {
    int result = 0;
    if (n % 2 == 1) {
        for (int i = 0; i < n; i += 2) {
            result ^= arr[i];
        }
    }
    return result;
}

int main() {
    int t;
    scanf("%d", &t);
    while (t--) {
        int n;
        scanf("%d", &n);
        int arr[n];
        for (int i = 0; i < n; i++) {
            scanf("%d", &arr[i]);
        }
        printf("%d\n", sansaXor(arr, n));
    }
    return 0;
}
