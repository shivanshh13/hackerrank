#include <stdio.h>
#include <string.h>

#define MOD 1000000007

int main() {
    char n[1000001];
    scanf("%s", n);
    int len = strlen(n);
    long long total = 0, f = 1, res = 0;
    for (int i = len - 1; i >= 0; i--) {
        long long num = n[i] - '0';
        res = (res + num * f * (i + 1)) % MOD;
        f = (f * 10 + 1) % MOD;
    }
    printf("%lld\n", res);
    return 0;
}
